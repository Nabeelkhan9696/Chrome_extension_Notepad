{
  "manifest_version": 2,
  "name": "Notes extension",
  "version": "1.0",
  "description": "A simple Chrome extension to take notes while browsing webpages.",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Webpage Notes"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
